{
  "name": "@hksagentur/avancarte",
  "version": "1.0.0",
  "description": "Website for AvanCarte based on Eleventy",
  "repository": "dweidner/eleventy-starter-kit",
  "author": "Daniel Weidner <d.weidner@hks-agentur.de>",
  "license": "ISC",
  "browser": "dist/assets/scripts/main.legacy.js",
  "module": "dist/assets/scripts/main.modern.js",
  "style": " dist/assets/styles/main.css",
  "private": true,
  "keywords": [
    "babel",
    "eleventy",
    "eslint",
    "postcss",
    "rollup",
    "stylelint"
  ],
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "browserslist": {
    "modern": [
      "last 2 Chrome versions",
      "not Chrome < 60",
      "last 2 Safari versions",
      "not Safari < 10.1",
      "last 2 iOS versions",
      "not iOS < 10.3",
      "last 2 Firefox versions",
      "not Firefox < 54",
      "last 2 Edge versions",
      "not Edge < 15"
    ],
    "legacy": [
      "> 1%",
      "not IE 11",
      "not op_mini all",
      "not dead"
    ]
  },
  "scripts": {
    "start": "run-s --silent clean sprite serve",
    "build": "run-s --silent clean sprite eleventy",
    "serve": "run-p --silent serve:*",
    "serve:eleventy": "eleventy --serve",
    "serve:cms": "cross-env PORT=3000 netlify-cms-proxy-server",
    "lint": "run-s --silent lint:*",
    "lint:scripts": "eslint .",
    "lint:styles": "stylelint 'src/assets/styles/**/*.css'",
    "clean": "rimraf dist",
    "eleventy": "cross-env NODE_ENV=production eleventy",
    "sprite": "svg-sprite --dest dist/assets --symbol --symbol-dest images --symbol-sprite icons.svg src/assets/icons/*.svg"
  },
  "dependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@11ty/eleventy-img": "^2.0.1",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@babel/core": "^7.17.8",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@csstools/postcss-is-pseudo-class": "^2.0.2",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "autoprefixer": "^10.4.4",
    "core-js": "^3.21.1",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.7",
    "dotenv": "^16.0.0",
    "dynamic-import-polyfill": "^0.1.1",
    "html-minifier": "^4.0.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "luxon": "^2.3.1",
    "markdown-it": "^13.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.13",
    "postcss-calc": "^8.2.4",
    "postcss-color-functional-notation": "^4.2.2",
    "postcss-custom-media": "^8.0.0",
    "postcss-custom-properties": "^12.1.6",
    "postcss-extend-rule": "^4.0.0",
    "postcss-gap-properties": "^3.0.3",
    "postcss-image-set-function": "^4.0.6",
    "postcss-import": "^14.1.0",
    "postcss-inset": "^1.0.0",
    "postcss-load-config": "^3.1.4",
    "postcss-nesting": "^10.1.4",
    "postcss-place": "^7.0.4",
    "postcss-url": "^10.1.3",
    "pretty-bytes": "^5.6.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "rollup-plugin-terser": "^7.0.2",
    "slugify": "^1.6.5",
    "svg-sprite": "^1.5.4",
    "swiper": "^8.1.4",
    "whatwg-fetch": "^3.6.2",
    "wicked-elements": "^3.1.2"
  },
  "devDependencies": {
    "eslint": "^8.12.0",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^38.1.6",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "netlify-cms-proxy-server": "^1.3.24",
    "stylelint": "^14.6.1",
    "stylelint-config-standard": "^25.0.0"
  }
}
