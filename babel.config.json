{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": "3.21",
        "loose": true
      }
    ]
  ],
  "env": {
    "modern": {
      "browserslistEnv": "modern",
      "targets": {
        "esmodules": true
      },
      "plugins": [
        "@babel/plugin-proposal-nullish-coalescing-operator",
        "@babel/plugin-proposal-optional-chaining"
      ]
    },
    "legacy": {
      "browserslistEnv": "legacy",
      "targets": {
        "esmodules": false
      }
    }
  }
}

